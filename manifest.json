{
    "manifest_version": 3,
    "name": "Instagram Reels Auto-Skip",
    "version": "1.0",
    "description": "Automatically skip to the next Instagram Reel after watching twice.",
    "host_permissions": [
      "https://www.instagram.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.instagram.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  